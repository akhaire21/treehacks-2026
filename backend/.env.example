# Anthropic (Claude) - Required for AI agent functionality
ANTHROPIC_API_KEY=your-anthropic-api-key-here
CLAUDE_MODEL=claude-sonnet-4-20250514
CLAUDE_MAX_TOKENS=4096

# JINA Embeddings - Required for semantic search
JINA_API_KEY=your-jina-api-key-here
JINA_MODEL=jina-embeddings-v3
JINA_EMBEDDING_DIM=1024

# Elasticsearch (Elastic Cloud) - Required for workflow search
# Get your Cloud ID from: https://cloud.elastic.co/deployments
ELASTIC_CLOUD_ID=your-deployment
ELASTIC_API_KEY=your-elastic-api-key-here
ELASTIC_INDEX=workflows

